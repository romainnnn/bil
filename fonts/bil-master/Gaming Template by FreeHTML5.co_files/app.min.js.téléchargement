!function(t,a,e){console.log("init");var n={init:function(e){n.select2(),n.owlCarousel(),n.portfolio(),n.lightCase(),n.counter(),n.scrollTo(),n.navbarChange(),n.validationForm()},select2:function(){t(e).ready(function(){t("select").select2()})},owlCarousel:function(){t(".slideshow").owlCarousel({loop:!0,items:1,nav:!0,navSpeed:1e3,dots:!0,dotSpeed:1e3,autoplay:!0,autoplaySpeed:1e3,autoplayTimeout:5e3,navText:['<span class="icon-nav-slider nav-left"></span>','<span class="icon-nav-slider nav-right"></span>']})},portfolio:function(){var a=t(".grid-portfolio").isotope({itemSelector:".grid-item",masonry:{gutter:".gutter-sizer",columnWidth:".grid-sizer"},percentPosition:!0});return t(".filter-button-group").on("click","a",function(){var e=t(this).attr("data-filter");a.isotope({filter:e})}),t(".btn-filter a.is-checked").addClass("active"),t(".btn-filter a").on("click",function(){t(".btn-filter a").removeClass("active"),t(this).addClass("active")}),!1},lightCase:function(){jQuery(e).ready(function(e){e("a[data-rel^=lightcase]").lightcase()})},counter:function(){t("#counter").each(function(){t(this).waypoint({handler:function(e){t(".number").countTo({speed:1e3}),this.destroy()},offset:"80%"})})},scrollTo:function(){t("#navbar-nav-header .nav-link").on("click",function(e){var a,n=t(this).attr("href");a="#home"!==n?t(n).offset().top-70:0,t("html, body").animate({scrollTop:a},750),e.preventDefault()})},navbarChange:function(){t(a).scroll(function(){var e=t(a).scrollTop();return 150<e?(t("nav").removeClass("navbar-transparent"),t("body").addClass("not-on-top")):(t("body").removeClass("not-on-top"),t("nav").addClass("navbar-transparent")),!1})},validationForm:function(){t("form[name='contact-us']").validate({rules:{name:{required:!0,minlength:3,maxlength:25},email:{required:!0,email:!0},subject:{required:!0,minlength:3,maxlength:24},message:{required:!0,minlength:5}},messages:{name:"Please enter your name at least 3 characters",subject:"Please enter your subject at least 3 characters",message:{required:"Please provide your message",minlength:"Your message must be at least 5 characters long"},email:{required:"Please enter a valid email address",email:"Your email address must be in the format of name@domain.com"}},submitHandler:function(e){var a=t(".submitting");t.ajax({type:"POST",url:"php/sendEmail.php",data:t(e).serialize(),beforeSend:function(){a.css("display","block").text("Submitting...")},success:function(e){"OK"==e?(t("#form-message-warning").hide(),setTimeout(function(){t("#contactForm").fadeOut()},1e3),setTimeout(function(){t("#form-message-success").fadeIn()},1400)):(t("#form-message-warning").html(e),t("#form-message-warning").fadeIn(),a.css("display","none"))},error:function(){t("#form-message-warning").html("Something went wrong. Please try again."),t("#form-message-warning").fadeIn(),a.css("display","none")}})}})}};t(e).ready(function(){n.init(t)})}(window.jQuery,window,document);